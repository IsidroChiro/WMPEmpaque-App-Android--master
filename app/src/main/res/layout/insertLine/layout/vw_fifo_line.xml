                            android:layout_height="wrap_content"
                            android:text="Lugar"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
