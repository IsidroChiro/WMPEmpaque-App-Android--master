<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_margin="0dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/designfifo"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.7">

                    <TextView
                        android:id="@+id/tv_Folio"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Folio"
                        android:textSize="16dp"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:textColor="#2978B5"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="0.3">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_weight="0.5">

                        <TextView
                            android:id="@+id/tv_Boxes"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Cajas"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"
                            android:layout_weight="0.5"/>

                        <TextView
                            android:id="@+id/tv_SKU"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="SKU"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"
                            android:layout_weight="0.5"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="5dp"
                        android:background="#707070"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5">

                        <TextView
                            android:id="@+id/tv_Place"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Planta"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="5dp"
                        android:background="#707070"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_lineDate"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Fecha Ingreso"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"
                            android:layout_weight="0.5"/>


                        <TextView
                            android:id="@+id/tv_nameLine"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Linea"
                            android:textSize="15dp"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textColor="#707070"
                            android:layout_weight="0.5"/>

                    </LinearLayout>



                </LinearLayout>



            </LinearLayout>





        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="5dp"
            android:layout_marginTop="10dp"
            android:background="#f4d101"/>

    </LinearLayout>





</FrameLayout>
