<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="15dp">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="fill_parent">

            <LinearLayout
                android:layout_gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left"
                            android:text="Pallet: "
                            android:textSize="25dp"
                            android:textColor="#000"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:imeOptions="flagNoExtractUi"
                            android:id="@+id/tv_pallet"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="25dp"
                            android:text="Pallet"
                            android:textColor="#000"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left"
                            android:text="Calidad: "
                            android:textSize="25dp"
                            android:textColor="#000"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:textColor="#000"
                            android:id="@+id/tv_calidad"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Calidad"
                            android:textSize="25dp"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">
                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Status del pallet: "
                            android:textSize="25dp"
                            android:textColor="#000"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Status del pallet"
                            android:textSize="25dp"
                            android:textColor="#000"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Producto:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_producto"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Producto"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Carta porte:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_cartaPorte"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Carta Porte"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Planta:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_planta"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Planta"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="SKU:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_sku"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="SKU"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Estado de calidad:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_StatusQuality"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Estado de calidad"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Paso por precooler:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_precooler"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Pas por precooler"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Cajas Totales:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_cajasTotales"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Cajas Totales"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Horas Totales:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_totalHours"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Horas totales"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="@dimen/folioPalletInfo"
                        android:orientation="horizontal">

                        <TextView
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Listo para embarque:"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_embarque"
                            android:gravity="left"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Listo para embarque"
                            android:textSize="25dp"
                            android:textColor="#000" />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

    </LinearLayout>

</LinearLayout>
